<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="439.0" prefWidth="652.0" xmlns="http://javafx.com/javafx/9.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="trip.TripOrganize">
    <ImageView fitHeight="442.0" fitWidth="654.0" pickOnBounds="true" preserveRatio="true">
        <image>
          <Image url="@../../image/185235620.jpg"/>
        </image>
    </ImageView>
    <TableView fx:id="tableGita" layoutX="107.0" layoutY="90.0" prefHeight="218.0" prefWidth="445.0">
      <columns>
          <TableColumn fx:id="columnIDGita" prefWidth="75.0" text="ID"/>
          <TableColumn fx:id="columnMeta" prefWidth="75.0" text="Meta"/>
          <TableColumn fx:id="columnAndata" prefWidth="100.0" text="Andata"/>
          <TableColumn fx:id="columnRitorno" prefWidth="94.0" text="Ritorno"/>
          <TableColumn fx:id="colonnaPullman" prefWidth="100.0" text="Pullman"/>
      </columns>
    </TableView>
    <Button layoutX="107.0" layoutY="333.0" mnemonicParsing="false" onAction="#viewInfo" prefHeight="35.0"
            prefWidth="86.0" text="Mostra Gite"/>
    <Button layoutX="284.0" layoutY="333.0" mnemonicParsing="false" onAction="#createTrip" prefHeight="35.0"
            prefWidth="86.0" text="Crea Gita"/>
    <Button layoutX="466.0" layoutY="333.0" mnemonicParsing="false" onAction="#deleteTrip" prefHeight="35.0"
            prefWidth="86.0" text="Rimuovi Gita"/>
    <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="36.0"/>
</AnchorPane>
